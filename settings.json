{
  "http": {
    "ip": "localhost",
    "port": 80
  },
  "https": {
	"ip": "localhost",
	"port": 443,
	"key": "/certificates/localhost-private.pem",
	"cert": "/certificates/localhost.crt"
  },
  "redirect": "/public/redirect",
  "paths": [
  
    {
      "virtual": "/",
      "real": "/public/client",
      "static": {
        "maxage": 86400000
      }
    },
    
    {
      "virtual": "/res",
      "real": "/public/res",
      "static": {
        "maxage": 86400000
      }
    },
    
    {
      "virtual": "/host/create",
      "real": "/public/create",
      "static": {
        "maxage": 86400000
      }
    },
    
    {
      "settings": "/server/router/host.js"
    }
    
  ]
  
}