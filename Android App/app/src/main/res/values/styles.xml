<?xml version="1.0" encoding="utf-8" ?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <!-- [Common styles]  -->
  <style name="match">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">match_parent</item>
  </style>

  <style name="wrap">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
  </style>

  <style name="recycler_view" parent="match">
    <item name="android:scrollbars">vertical</item>
  </style>

  <style name="fragment" parent="match">
    <item name="android:layout_gravity">start</item>
  </style>

  <style name="row">
    <item name="android:layout_width">fill_parent</item>
    <item name="android:layout_height">wrap_content</item>
  </style>
  <!-- [/Common styles] -->


  <!-- [DrawerArrow]  -->
  <style name="drawer_arrow_style" parent="Widget.AppCompat.DrawerArrowToggle">
    <item name="spinBars">true</item>
    <item name="color">?drawer_arrow_color</item>
  </style>
  <!-- [/DrawerArrow] -->


  <!-- [Styles for main_layout.xml]  -->

  <!-- [include]  -->
  <style name="include_style" parent="wrap" />
  <!-- [/include] -->


  <!-- [DrawerLayout]  -->
  <style name="drawer_style" parent="match" />
  <!-- [/DrawerLayout] -->


  <!-- [FrameLayout]  -->
  <style name="frame_layout_style" parent="match">
    <item name="android:background">#000000</item>
  </style>
  <!-- [/FrameLayout] -->


  <!-- [fragment]  -->
  <style name="fragment_drawer_style" parent="fragment">
    <item name="android:layout_width">?drawer_width</item>
  </style>
  <!-- [/fragment] -->

  <!-- [/Styles for main_layout.xml] -->


  <!-- [Styles for toolbar.xml]  -->

  <!-- [Toolbar] -->
  <style name="toolbar_style" parent="wrap">
    <item name="android:background">?toolbar_background</item>
    <item name="android:textColor">?toolbar_color</item>
    <item name="android:minHeight">@dimen/abc_action_bar_default_height_material</item>
    <item name="popupTheme">@style/ThemeOverlay.AppCompat.Light</item>
    <item name="theme">@style/ThemeOverlay.AppCompat.Dark.ActionBar</item>
  </style>
  <!-- [/Toolbar] -->

  <!-- [/Styles for toolbar.xml] -->


  <!-- [Styles for navigation_drawer_fragment.xml (some may be reusable)]  -->

  <!-- [RecyclerView]  -->
  <style name="drawer_recycler_view_style" parent="recycler_view">
    <item name="android:background">?drawer_background</item>
    <item name="android:focusable">true</item>
    <item name="android:clickable">true</item>
    <item name="android:focusableInTouchMode">true</item>
  </style>
  <!-- [/RecyclerView] -->

  <!-- [/Styles for navigation_drawer_fragment.xml] -->


  <!-- [Styles for drawer_item_row.xml]  -->

  <!-- [LinearLayout]  -->
  <style name="drawer_layout_style">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">?drawer_item_height</item>
  </style>
  <!-- [/LinearLayout] -->


  <!-- [TextView]  -->
  <style name="drawer_item_style.base" parent="match">
    <item name="android:layout_marginRight">?drawer_item_margin_right</item>
    <item name="android:layout_marginLeft">?drawer_item_margin_left</item>
    <item name="android:drawablePadding">?drawer_item_icon_padding</item>
    <item name="android:textSize">?drawer_item_text_size</item>
    <item name="android:textColor">?drawer_item_text_color</item>
    <item name="android:gravity">center_vertical</item>
  </style>

  <style name="drawer_item_style" parent="drawer_item_style.base" />
  <!-- [/TextView] -->

  <!-- [/Styles for drawer_item_row.xml] -->


  <!-- [Styles for progress_dialog.xml]  -->

  <!-- [LinearLayout]  -->
  <style name="progress_dialog_layout_style" parent="wrap">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:orientation">horizontal</item>
    <item name="android:layout_gravity">center</item>
    <item name="android:background">@drawable/progress_dialog</item>
  </style>
  <!-- [/LinearLayout] -->

  <!-- [ProgressBarCircularIIndeterminate]  -->
  <style name="progress_bar_style">
    <item name="android:layout_width">?progress_circular_size</item>
    <item name="android:layout_height">?progress_circular_size</item>
    <item name="android:layout_gravity">center_horizontal</item>
    <item name="android:background">?progress_circular_color</item>
  </style>
  <!-- [/ProgressBarCircularIIndeterminate] -->

  <!-- [TextView]  -->
  <style name="progress_dialog_text_style" parent="match">
    <item name="android:layout_width">wrap_content</item>
    <item name="android:textColor">#333333</item>
    <item name="android:gravity">center_vertical</item>
    <item name="android:layout_marginLeft">15dp</item>
    <item name="android:layout_marginRight">8dp</item>
  </style>
  <!-- [/TextView] -->

  <!-- [/Styles for progress_dialog.xml] -->


  <!-- [Styles for preference_layout.xml]  -->

  <!-- [RelativeLayout]  -->
  <!-- [/RelativeLayout] -->


  <!-- [fragment]  -->
  <!-- [/fragment] -->

  <!-- [/Styles for preference_layout.xml] -->


  <!-- [Styles for preference_fragment.xml]  -->
  <!-- [/Styles for preference_fragment.xml] -->


  <!-- [Styles for bluetooth_enable.xml]  -->

  <!-- [LinearLayout]  -->
  <style name="bluetooth_layout_style" parent="match">
    <item name="android:orientation">vertical</item>
    <item name="android:gravity">center</item>
    <item name="android:background">#FFFFFF</item>
  </style>
  <!-- [/LinearLayout] -->

  <!-- [TextView]  -->
  <style name="bluetooth_message_style" parent="wrap">
    <item name="android:textColor">?bluetooth_message_text_color</item>
    <item name="android:textSize">?bluetooth_message_text_size</item>
    <item name="android:gravity">center</item>
  </style>
  <!-- [/TextView] -->

  <!-- [Button]  -->
  <style name="bluetooth_button_style" parent="wrap">
    <item name="android:layout_width">wrap_content</item>
  </style>
  <!-- [/Button] -->

  <!-- [/Styles for bluetooth_enable.xml] -->


  <!-- [Styles for scan_fragment.xml (some may be reusable)]  -->

  <!-- [RelativeLayout]  -->
  <style name="relative_layout_style" parent="match">
    <item name="android:orientation">horizontal</item>
    <item name="android:gravity">center_vertical</item>
    <item name="android:background">#FFFFFF</item>
  </style>
  <!-- [/RelativeLayout] -->


  <!-- [RecyclerView]  -->
  <!-- [/RecyclerView] -->


  <!-- [ButtonFloat]  -->
  <style name="float_button_style">
    <item name="android:layout_width">?button_float_radius</item>
    <item name="android:layout_height">?button_float_radius</item>
    <item name="android:layout_marginRight">?button_float_margin</item>
    <item name="android:layout_marginBottom">?button_float_margin</item>
    <item name="android:background">?button_float_background</item>
    <item name="android:layout_alignParentRight">true</item>
    <item name="android:layout_alignParentBottom">true</item>
  </style>
  <!-- [/ButtonFloat] -->

  <!-- [/Styles for scan_fragment.xml (and home_fragment.xml)] -->


  <!-- [Styles for paired_device.xml]  -->

  <!-- [RippleView] -->
  <style name="device_ripple_style" parent="wrap">
    <item name="rv_color">?paired_device_ripple_color</item>
    <item name="rv_rippleDuration">?paired_device_ripple_duration</item>
  </style>
  <!-- [/RippleView] -->

  <!-- [LinearLayout]  -->
  <style name="device_layout_style" parent="match">
    <item name="android:layout_height">?paired_device_height</item>
    <item name="android:layout_margin">?paired_device_padding</item>
  </style>
  <!-- [/LinearLayout] -->


  <!-- [TextView]  -->
  <style name="device_signal_style" parent="match">
    <item name="android:layout_width">?paired_device_height</item>
    <item name="android:textColor">?paired_device_signal_text_color</item>
    <item name="android:textSize">?paired_device_signal_text_size</item>
    <item name="android:background">?paired_device_signal_shape</item>
    <item name="android:gravity">center</item>
  </style>
  <!-- [/TextView] -->


  <!-- [LinearLayout]  -->
  <style name="device_info_style" parent="row">
    <item name="android:layout_height">match_parent</item>
    <item name="android:layout_marginLeft">?paired_device_info_margin</item>
    <item name="android:orientation">vertical</item>
    <item name="android:gravity">center</item>
  </style>
  <!-- [/LinearLayout] -->


  <!-- [TextView]  -->
  <style name="device_name_style" parent="row">
    <item name="android:textSize">?paired_device_name_text_size</item>
    <item name="android:textColor">?paired_device_name_text_color</item>
    <item name="android:layout_toRightOf">@+id/paired_device_connection</item>
  </style>
  <!-- [/TextView] -->


  <!-- [TextView]  -->
  <style name="device_address_style" parent="row">
    <item name="android:textSize">?paired_device_address_text_size</item>
    <item name="android:textColor">?paired_device_address_text_color</item>
    <item name="android:layout_toRightOf">@+id/paired_device_connection</item>
    <item name="android:layout_below">@+id/paired_device_name</item>
  </style>
  <!-- [/TextView] -->

  <!-- [/Styles for paired_device.xml] -->


  <!-- [Base style for theme (common metrics in all themes)]  -->
  <style name="AppTheme.Base" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="drawerArrowStyle">@style/drawer_arrow_style</item>
    <item name="android:textColor">#000000</item>
    <item name="android:windowBackground">@color/window_background</item>

    <!-- [Styles that applies to styles defined below]  -->
    <item name="drawer_layout">@layout/navigation_drawer_fragment</item>
    <item name="drawer_width">@dimen/drawer_width</item>
    <item name="drawer_background">@color/drawer_background</item>
    <item name="drawer_item_height">@dimen/drawer_item_height</item>

    <item name="drawer_item_margin_start">@dimen/drawer_item_margin</item>
    <item name="drawer_item_margin_right">@dimen/drawer_item_margin</item>
    <item name="drawer_item_margin_end">@dimen/drawer_item_margin</item>
    <item name="drawer_item_margin_left">@dimen/drawer_item_margin</item>

    <item name="drawer_item_icon_padding">@dimen/drawer_item_icon_padding</item>
    <item name="drawer_item_text_size">@dimen/drawer_item_size</item>
    <item name="drawer_item_text_color">@color/drawer_item_color</item>
    <item name="drawer_item_alpha">@dimen/drawer_item_alpha</item>

    <item name="bluetooth_message_text_color">@color/bluetooth</item>
    <item name="bluetooth_message_text_size">@dimen/bluetooth</item>

    <item name="button_float_radius">@dimen/button_radius</item>
    <item name="button_float_margin">@dimen/button_margin</item>

    <item name="progress_circular_size">@dimen/progress_circular_size</item>

    <item name="paired_device_height">@dimen/device_height</item>
    <item name="paired_device_padding">@dimen/device_padding</item>
    <item name="paired_device_ripple_color">@color/paired_device_ripple_color</item>
    <item name="paired_device_ripple_duration">@integer/paired_device_ripple_duration</item>
    <item name="paired_device_signal_text_color">@color/paired_device_signal</item>
    <item name="paired_device_signal_text_size">@dimen/device_signal_size</item>
    <item name="paired_device_info_margin">@dimen/device_info_margin</item>
    <item name="paired_device_name_text_size">@dimen/device_name_size</item>
    <item name="paired_device_address_text_size">@dimen/device_address_size</item>
    <item name="paired_device_name_text_color">@color/paired_device_name</item>
    <item name="paired_device_address_text_color">@color/paired_device_address</item>
    <!-- [/Styles that applies to styles defined below] -->

  </style>

  <style name="AppTheme" parent="AppTheme.Base" />
  <!-- [/Base style for theme (common metrics in all themes)] -->


